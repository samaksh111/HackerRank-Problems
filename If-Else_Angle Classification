#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int deg;
    scanf("%d",&deg);
    if(deg>360 && deg%360!=0)
    {       
    deg=deg%360 ;
    } 
    if(deg%360==0 && deg>360)
    {
        printf("Full Rotation");
    }
    if(deg<90)
    {
        printf("Acute Angle");
    }
     if(deg==90)
    {
        printf("Right Angle");
    }
    if(deg==180)
    {
        printf("Straight Angle");
    }
     else if (deg==360)
    {
        printf("Full Rotation");
    }
    if(deg>90 && deg<180)
    {
        printf("Obtuse Angle");
    }
    if (deg>180 && deg<360)
    {
        printf("Reflex Angle");
    }
    
    
    return 0;
}

